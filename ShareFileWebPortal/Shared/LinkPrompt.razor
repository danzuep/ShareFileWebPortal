<div class="alert alert-secondary mt-4">
    <span class="oi oi-pencil me-2" aria-hidden="true"></span>
    <strong>@Title</strong>

    <span class="text-nowrap">
        Click
        <a target="_blank" class="font-weight-bold link-dark" href="https://example.sharefile.com/">here</a>
    </span>
    to log in to ShareFile directly.
</div>

@code {
    [Parameter]
    public string? Title { get; set; }
}
